{
  "name": "lolisleep.moe",
  "version": "1.0.0",
  "description": "Soft loli breathing to help you sleep.",
  "main": "dist/server.js",
  "scripts": {
    "start": "npm run postinstall && npm run web",
    "web": "node --max_old_space_size=4096 dist/server.js",
    "postinstall": "npm run clean && npm run bundle-web && npm run bundle-server",
    "bundle-web": "webpack --env platform=web",
    "bundle-server": "webpack --env platform=server",
    "clean": "del-cli ./dist"
  },
  "keywords": ["loli", "sleep", "lolisleep.moe"],
  "author": "Tenpi",
  "license": "MIT",
  "devDependencies": {
    "@tenpi/tslint": "^1.1.0",
    "tslint": "^6.1.3"
  },
  "dependencies": {
    "@types/express": "^4.17.11",
    "@types/less": "^3.0.2",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-router-dom": "^5.1.7",
    "@types/webpack-env": "^1.16.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "css-loader": "^5.2.1",
    "del-cli": "^3.0.1",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^7.0.2",
    "express": "^4.17.1",
    "express-favicon": "^2.0.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.1",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "less": "^4.1.1",
    "less-loader": "^8.1.0",
    "mini-css-extract-plugin": "^1.4.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.4",
    "webpack": "^5.31.2",
    "webpack-cli": "^4.6.0",
    "webpack-dev-middleware": "^4.1.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^2.5.2"
  }
}
